@using Minimarket_Raphi.Models
@using System.Data.Entity.Core.Objects;
@model IEnumerable<Minimarket_Raphi.Models.Hoja_de_Registro>
@{
    ViewBag.Title = "NuevaHoja_de_Registro";
}
<div class="jumbotron">
    <h2>Nueva Hoja de Registro</h2>
    @using (Html.BeginForm("NuevaHoja_de_Registro", "Home", FormMethod.Post))
    {
        <style>
            div {
                margin-bottom: 10px;
            }

            label {
                display: inline-block;
                width: 150px;
                text-align: right;
            }
        </style>
        @Html.Label("Ingrese ID Kardex:")
        @Html.TextBox("ID_Kardex")
        <br>
        @Html.Label("Ingrese Código Empleado:")
        @Html.TextBox("Codigo_Empleado")
        <br>
        @Html.Label("Ingrese ID HojaRegistro:")
        @Html.TextBox("ID_HojaRegistro")
        <br>
        @Html.Label("Ingrese Saldo Final Mensual:")
        @Html.TextBox("Saldo_Final_Mensual")
        <br>
        @Html.Label("Ingrese Área:")
        @Html.TextBox("Area")
        <br>
        @Html.Label("Ingrese Turno:")
        @Html.TextBox("Turno")
        <br>
        @Html.Label("Ingrese DÍa")
        @Html.TextBox("Dia")
        <br>
        @Html.Label("Ingrese Mes:")
        @Html.TextBox("Mes")
        <br>
        @Html.Label("Ingrese Año:")
        @Html.TextBox("Anio")
        <br>



        <input type="submit" class="btn btn-success" value="Enviar">

    }
</div>